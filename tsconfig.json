{
  "compilerOptions": {
    "declaration": true,
    "noImplicitThis": true,
    "outDir": "dist",
    "target": "esnext",
    "useDefineForClassFields": true,
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "lib": [
      "esnext",
      "dom"
    ],
    "skipLibCheck": true,
    "noImplicitAny": true,
  },
  "include": [
    "recursive_stack.ts"
  ],
}
